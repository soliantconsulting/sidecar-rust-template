[package]
name = "{{ project-name }}"
version = "0.1.0"
edition = "2021"

[dependencies]
lambda_http = "0.14.0"
lambda_runtime = { version = "0.13.0", features = ["anyhow"] }
aws_lambda_events = { version = "0.16.0", default-features = false, features = ["sqs"] }
tokio = { version = "1", features = ["macros"] }
async-trait = "0.1.88"
serde = { version = "1.0.219", features = ["derive"] }
serde_json = "1.0.140"
anyhow = "1.0.97"
aws-config = "1.6.0"
aws-sdk-sqs = "1.62.0"
aws-sdk-ssm = "1.68.0"
base64 = "0.22.1"
fm-script-client = { version = "1.2.1", default-features = false, features = ["rustls-tls"] }
serde_path_to_error = "0.1.17"
thiserror = "2.0.12"
serde_html_form = "0.2.7"
